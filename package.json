{
  "name": "commutox",
  "sideEffects": [
    "*.{scss,css}"
  ],
  "version": "1.0.0",
  "description": "A ReactJS messenger",
  "private": true,
  "scripts": {
    "test": "jest --runInBand",
    "test:watch": "jest --runInBand --watch",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "start": "webpack-dev-server --config webpack/webpack.dev.js --progress --color",
    "build": "webpack --config webpack/webpack.prod.js -p --progress --colors",
    "docz:dev": "docz dev",
    "docz:build": "docz build",
    "tslint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "tsc": "tsc --noEmit",
    "prettier": "prettier --config ./.prettierrc \"**/*.+(js|jsx|ts|tsx|json|yaml|yml|css|sass|scss|md|mdx|graphql)\"",
    "format": "npm run prettier -- --write",
    "validate": "npm run tslint && npm run prettier -- --list-different && npm run tsc"
  },
  "keywords": [
    "messenger",
    "chat",
    "telegram",
    "whatsapp",
    "communication"
  ],
  "author": "Alexey Ryabov",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.2.0",
    "@types/classnames": "^2.2.6",
    "@types/jest": "^23.3.10",
    "@types/node": "^10.12.11",
    "@types/react": "^16.7.20",
    "@types/react-dom": "^16.0.11",
    "@types/react-redux": "^6.0.10",
    "@types/react-router-dom": "^4.3.1",
    "@types/webpack": "^4.4.20",
    "@types/webpack-env": "^1.13.6",
    "@types/yup": "^0.26.7",
    "autoprefixer": "^9.3.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^1.0.0",
    "css-loader": "^1.0.1",
    "cypress": "^3.1.3",
    "docz": "^0.13.7",
    "docz-theme-default": "^0.13.7",
    "eslint": "^5.9.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "mini-css-extract-plugin": "^0.4.5",
    "node-sass": "^4.10.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.4.0",
    "prettier": "^1.15.3",
    "react-hot-loader": "^4.6.3",
    "react-testing-library": "^5.3.1",
    "redux-devtools-extension": "^2.13.7",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.1",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.2",
    "typings-for-css-modules-loader": "^1.7.0",
    "webpack": "^4.26.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "classnames": "^2.2.6",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^5.1.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "yup": "^0.26.10"
  }
}
