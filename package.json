{
  "name": "commutox",
  "sideEffects": [
    "./src/polyfills.ts",
    "*.{scss,css}"
  ],
  "version": "1.0.0",
  "description": "A ReactJS messenger",
  "private": true,
  "scripts": {
    "test": "jest --runInBand",
    "test:watch": "jest --runInBand --watch",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "start": "webpack-dev-server --config webpack/webpack.dev.js --progress --color",
    "prebuild": "npm run validate",
    "build": "webpack --config webpack/webpack.prod.js -p --progress --colors",
    "docz:dev": "docz dev",
    "docz:build": "docz build",
    "tslint": "tslint -p tsconfig.json -c tslint.json",
    "typecheck": "tsc --noEmit",
    "prettier": "prettier --config ./.prettierrc \"**/!(*.?(s)css.d).+(js|jsx|ts|tsx|json|yaml|yml|css|sass|scss|md|mdx|graphql)\"",
    "format": "npm run prettier -- --write",
    "validate": "npm run tslint && npm run prettier -- --list-different && npm run typecheck",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "keywords": [
    "messenger",
    "chat",
    "telegram",
    "whatsapp",
    "communication"
  ],
  "author": "Alexey Ryabov",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.6.0",
    "@storybook/addon-actions": "^5.3.0-alpha.0",
    "@storybook/addon-docs": "^5.3.0-alpha.0",
    "@storybook/addon-links": "^5.3.0-alpha.0",
    "@storybook/addon-storysource": "^5.3.0-alpha.0",
    "@storybook/addon-viewport": "^5.3.0-alpha.0",
    "@storybook/addons": "^5.3.0-alpha.0",
    "@storybook/react": "^5.3.0-alpha.0",
    "@storybook/source-loader": "^5.3.0-alpha.0",
    "@storybook/theming": "^5.3.0-alpha.0",
    "@types/classnames": "^2.2.9",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.5",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-redux": "^7.1.2",
    "@types/react-router-dom": "^5.1.0",
    "@types/storybook__react": "^4.0.2",
    "@types/webpack": "^4.39.1",
    "@types/webpack-env": "^1.14.0",
    "@types/yup": "^0.26.23",
    "autoprefixer": "^9.6.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^2.0.2",
    "css-loader": "^3.2.0",
    "cypress": "^3.3.0",
    "cypress-testing-library": "^3.0.1",
    "dts-css-modules-loader": "^1.0.1",
    "file-loader": "^4.2.0",
    "fork-ts-checker-webpack-plugin": "^1.3.3",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.3.0",
    "jest": "^24.9.0",
    "lint-staged": "^8.1.7",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.18.2",
    "react-docgen-typescript-loader": "^3.3.0",
    "react-hot-loader": "^4.12.13",
    "react-testing-library": "^7.0.0",
    "redux-devtools-extension": "^2.13.8",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "tslint": "^5.20.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^4.0.0",
    "typescript": "^3.6.3",
    "webpack": "^4.40.2",
    "webpack-bundle-analyzer": "^3.5.0",
    "webpack-cli": "^3.3.8",
    "webpack-dev-server": "^3.8.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@hot-loader/react-dom": "^16.9.0",
    "classnames": "^2.2.6",
    "core-js": "^3.2.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.1.1",
    "redux": "^4.0.4",
    "yup": "^0.27.0"
  }
}
