    "style-loader": "^2.0.0",
    "typescript": "^4.0.5",
    "utility-types": "^3.10.0",
    "webpack": "^5.76.0",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.3.0"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "core-js": "^3.6.5",
    "query-string": "^6.13.7",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-relay": "^10.0.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "yup": "^0.29.3"
  }
}
