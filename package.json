{
  "name": "commutox",
  "sideEffects": [
    "./src/polyfills.ts",
    "*.{scss,css}"
  ],
  "version": "1.0.0",
  "description": "A ReactJS messenger",
  "private": true,
  "scripts": {
    "test": "jest --runInBand",
    "test:watch": "jest --runInBand --watch",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "cypress:open": "cypress open",
    "start": "webpack-dev-server --config webpack/webpack.dev.js --progress --color",
    "prebuild": "npm run validate",
    "build": "webpack --config webpack/webpack.prod.js -p --progress --colors",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "prettier": "prettier --config ./.prettierrc \"**/!(*.?(s)css.d).+(js|jsx|ts|tsx|json|yaml|yml|css|sass|scss|md|mdx|graphql)\"",
    "prettier:format": "npm run prettier -- --write",
    "prettier:check": "npm run prettier -- --check",
    "validate": "npm run lint && npm run prettier:check && npm run typecheck",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "keywords": [
    "messenger",
    "chat",
    "telegram",
    "whatsapp",
    "communication"
  ],
  "author": "Alexey Ryabov",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@babel/preset-typescript": "^7.10.1",
    "@storybook/addon-docs": "^5.3.19",
    "@storybook/addon-info": "^5.3.19",
    "@storybook/addon-storysource": "^5.3.19",
    "@storybook/addon-viewport": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/react": "^5.3.19",
    "@storybook/source-loader": "^5.3.19",
    "@storybook/theming": "^5.3.19",
    "@testing-library/cypress": "^6.0.0",
    "@testing-library/dom": "^7.9.0",
    "@testing-library/react": "^10.2.0",
    "@types/classnames": "^2.2.10",
    "@types/eslint": "^6.8.1",
    "@types/jest": "^25.2.3",
    "@types/node": "^12.12.43",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.5",
    "@types/testing-library__cypress": "^5.0.5",
    "@types/webpack": "^4.41.17",
    "@types/webpack-env": "^1.15.2",
    "@types/yup": "^0.29.2",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "autoprefixer": "^9.8.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.5.3",
    "cypress": "^4.7.0",
    "dts-css-modules-loader": "^1.0.3",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "eslint-plugin-unicorn": "^20.1.0",
    "file-loader": "^6.0.0",
    "fork-ts-checker-webpack-plugin": "^4.1.6",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.9",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.0.5",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-hot-loader": "^4.12.21",
    "redux-devtools-extension": "^2.13.8",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "typescript": "^3.9.3",
    "utility-types": "^3.10.0",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@hot-loader/react-dom": "^16.13.0",
    "classnames": "^2.2.6",
    "core-js": "^3.6.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "yup": "^0.29.1"
  }
}
